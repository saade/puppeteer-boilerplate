{
"update_url": "https://clients2.google.com/service/update2/crx",

  "version": "0.1.1",
  "manifest_version": 2,
  "short_name": "access-control-allow-origin",
  "name": "Allow CORS: Access-Control-Allow-Origin",
  "background": {"page": "lib/chrome/background.html"},
  "homepage_url": "http://mybrowseraddon.com/access-control-allow-origin.html",
  "permissions": ["storage", "<all_urls>", "webRequest", "webRequestBlocking"],
  "description": "Easily add (Access-Control-Allow-Origin: *) rule to the response header.",
  "commands": {
    "toggle-default-mode": {
      "description": "Access-Control-Allow-Origin",
      "suggested_key": {"default": "Ctrl+Shift+D", "mac": "Command+Shift+D"}
    }
  },
  "browser_action": {
    "default_title": "Access-Control-Allow-Origin",
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
    }
  },
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png"
  }
}
